\fancyhead[ER]{\nouppercase{\small Zmiany dla wersji \DIFrevdesc}}

\providecommand{\DIFauthor}{Korekta}
\providecommand{\DIFaddsub}{Wstawiono}
\providecommand{\DIFdelsub}{Skasowano}

\providecommand{\DIFaddsym}{$[+]$}
\providecommand{\DIFdelsym}{$[-]$}

\providecommand{\DIFadd}[1]{\pdfmarkupcomment[author = {\DIFauthor}, subject = {\DIFaddsub}, color = blue, opacity = 0.25]
	{\relax\ifmmode\else\DIFaddsym\fi}{\DIFaddsub:~\luaescapestring{#1}}{\protect\color{blue}\uwave{#1}}}

\providecommand{\DIFdel}[1]{\pdfmarkupcomment[author = {\DIFauthor}, subject = {\DIFdelsub}, color = red, opacity = 0.25]
	{\relax\ifmmode\else\DIFdelsym\fi}{\DIFdelsub:~\luaescapestring{#1}}{\protect\color{red}\sout{#1}}}

\providecommand{\DIFaddFL}[1]{\DIFadd{#1}}
\providecommand{\DIFdelFL}[1]{\DIFdel{#1}}

