@article{mehrnia_fir,
	title = {FIR filter design using optimal factoring: a walkthrough and summary of benefits},
	author = {Mehrnia, A. and Willson, A. N.},
	journal = {IEEE Circuits and Systems Magazine},
	volume = {16},
	number = {1},
